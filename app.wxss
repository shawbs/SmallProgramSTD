/**摇宝底部条**/
.opt-wp{
    display: flex;
    background: #4C4A4A;
    height: 60px;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 9;
    padding: 0 2%;
    box-sizing: border-box;
    align-items: center;
}
.opt-wp .re-btn{
  background: #9d9c9e;
  border-radius: 4px;
  color: #ffffff;
  font-size: 14px;
  padding: 0 15px;
  width: 46%;
}
.opt-wp .primary{
  background: #F2A421;
}
.opt-wp .point-tip{
    font-size: 12px;
    color: #ACA39D;
}
.opt-wp .point{
  font-size: 16px;
  color: #F2A421;
  font-weight: 800;
}
.opt-wp .control{
  margin-left: 2%;
  flex: 1;
  display: flex;
  justify-content: space-between;
}
.opt-wp .input-control{
  border-radius: 4px;
  background: #ffffff;
  box-shadow: 0 0 5px #e76434 inset;
  padding: 10px 8px;
  color: #333;
  font-size: 14px;
  flex: 1;
}


.yb-progress{
  width: 100%;
  padding: 10px;
  font-size: 14px;
}
.yb-progress ._title{
    display: flex;
    justify-content: space-between;  
}

.yb-progress .plan{
    margin-top: 10px;
    border-radius: 8px;
    background: #ccc;
}
.yb-progress .plan .bg-wp{
    background: #E76434;
    border-radius: 8px;
    height: 8px;
}
.list{
  position: relative;
