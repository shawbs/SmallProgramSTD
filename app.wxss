/**商品列表**/
.good-item{
  position: relative;
  background: #ffffff;
}
.good-item .done-icon{
  position: absolute;
  left: 3px;
  top: 55px;
  width: 73px;
  height: 79px;
  /* z-index: 1; */
}
.good-item .good-item-title{
    font-size: 14px;
    color: #333;
    background: #ffffff;
    padding: 8px;
    line-height: 20px;
    height: 35px;
    border-bottom: 3px solid #d89f55;
}
.good-item .good-item-img{
   width: 100%; 
   height: 32vw; 
}
.good-item .good-item-fd{
  padding: 8px;
  line-height: 1.5em;
}
.good-item .good-item-price{
  color: #e76434;
}
.good-item .good-item-fd {
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: gray;
}
.good-item .good-price{
  flex: 1;
  border-right: 1px solid #F4F4F4;
}

.good-item .good-item-fd .status-pic{
  width: 50px;
  height: 25px;
}
/**good-list end**/

/**商场侧面菜单**/
.side-menu{
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  right: 0;
  background: rgba(0,0,0,.6);
  z-index: 10;
}
.side-menu .side-menu-container{
  position: fixed;
  width: 60%;
  height: 100%;
  top: 0;
  right: 0;
  background: #ffffff;
  transform: translate(100%);
}
.side-menu .side-menu-hd{
  padding-top: 20px;
  line-height: 30px;
  color: #666666;
  font-size: 14px;
  padding: 0 10px;
}
.side-menu .menu-bd .menu-item{
  border: 1px solid #f2f2f2;
  border-radius: 4px;
  font-size: 14px;
  line-height: 2em;
  color: #666666;
  text-align: center;
}
.side-menu .menu-bd .menu-item.active{
  border: 1px solid #e5b577;
  color: #e5b577;
  background-color: #fff9e9;
}
.side-menu .menu-bottom-bar{
  position: absolute;
  bottom: 0;
  right: 0;
  width: 100%;
  height: 50px;
  line-height: 50px;
  background: #1F1C1A;
}
.side-menu .menu-bottom-bar .menu-ctrl-item{
  text-align: center;
  font-size: 16px;
  color: #ffffff;
}
.side-menu .menu-bottom-bar .closeBtn{
  background-color: #d99f55;
}

/**确认订单中顶部商品项**/
.order-item{
    background: #ffffff;
    display: flex;
    display: -webkit-flex;
    flex-wrap: wrap;
    margin-bottom: 10px;
}
.order-item .order-item-bd{
    display: flex;
    display: -webkit-flex;
    width: 100%;
    border: 1px solid #ddd;
}
.order-item .order-item-fd{
    padding: 5px 10px;
    /* width: 100%; */
    /* height: 30px; */
}
.order-item .order-item-fd .todetail{
    color: #E5B577;
    line-height: 30px;
}
.order-item .img{
    width: 45%;
    height: 120px;
    display: flex;
    align-items: center;
    background: #000;
}
.order-item .show-img img{
    width: 100%;
    min-height: 110px;
}
.order-item .show-text{
    color: #888;
    font-size: 12px;
    padding: 12px;
    width: 55%;
    position: relative;
    box-sizing: border-box;
}
.order-item .show-text .title{
    color: #444444;
    font-size: 16px;
    line-height: 1.2em;
    margin-bottom: 7px;
}
.order-item .show-text .residue{
    position: absolute;
    bottom: 10px;
    left: 12px;
    font-size: 20px;
    color: #E86433;
}

/**大的底部条**/
.max-bottom-bar{
    background: #4C4A4A;
    height: 60px;
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
    z-index: 1;
    padding: 10px 12px;
    box-sizing: border-box;
}
.max-bottom-bar .point-tip {
    font-size: 12px;
    color: #ACA39D;
}
.max-bottom-bar .point {
    font-size: 18px;
    line-height: 22px;
    color: #F2A421;
    margin-top: 2px;
}
.max-bottom-bar .confirm-btn{
    position: absolute;
    background: #F2A421;
    color: #ffffff;
    width: 130px;
    height: 60px;
    line-height: 60px;
    font-size: 18px;
    text-align: center;
    right: 0;
    top: 0;
}

.checkbox{
  position: relative;
  width: 30px;
  height: 30px;
}
.checkbox switch{
  opacity: 0;
}
.checkbox checkbox{
   position: absolute;
   opacity: 0; 
}
.checkbox image{
  width: 30px;
  height: 30px;
}

